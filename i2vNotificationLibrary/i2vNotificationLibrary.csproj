<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
        <PackageId>i2vNotificationLibrary</PackageId>
        <Title>CRUD notification signalR Library</Title>
        <Version>0.0.2-beta</Version>
        <Authors>Architrixs</Authors>
        <Product> </Product>
        <Description>CRUD notification sending to clients using signalR</Description>
        <PackageReadmeFile></PackageReadmeFile>
        <PackageIcon>subscribe.png</PackageIcon>
    </PropertyGroup>

    <ItemGroup>
      <None Include="C:\Users\i2vsys\Downloads\subscribe.png">
        <Pack>True</Pack>
        <PackagePath>\</PackagePath>
      </None>
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.AspNetCore.SignalR.Common" Version="6.0.24" />
      <PackageReference Include="Microsoft.AspNetCore.SignalR.Core" Version="1.1.0" />
    </ItemGroup>

</Project>
